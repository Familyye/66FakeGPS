package com.carlex.drive;
import android.util.Log;


public class xGnssStatus {
    private int mSvCount;
    private int mBitMask;
    private int mTypes;
    private float mElevationDegrees;
    private float mAzimuthDegrees;
    private float mSnrInDb;
    private int mAlmanacHealth;
    private int mSvHealth;
    private int mCn0DbHz;
    private int mFlags;
    private int mSatelliteCount;
    private float mSnr;
    private int mConstellationType;
    private float mCarrierFrequencyHz;
    private boolean mHasEphemerisData;
    private boolean mUsedInFix;
    private boolean mHasAlmanacData;
	
    public xGnssStatus() {
        mSvCount = 0;
        mBitMask = 0;
        mTypes = 0;
        mElevationDegrees = (float) 0.0;
        mAzimuthDegrees = (float) 0.0;
        mSnrInDb = (float) 0.0;
        mAlmanacHealth = (int) 0;
        mSvHealth = (int) 0;
        mCn0DbHz = (int) 0;
        mFlags = (int) 0;
        mSatelliteCount = 0;
        mSnr = (float) 0.0;
        mConstellationType = (int) 0;
        mCarrierFrequencyHz = (float) 0.0;
        mHasEphemerisData = (boolean) false;
	mHasAlmanacData = (boolean) false;
        mUsedInFix = (boolean) false;
    }

    private static final String TAG = "xGnssStatus";


    // Métodos para obter informações do status dos satélites GNSS
    public int getSatelliteCount() {
        Log.d(TAG, "getSatelliteCount() called");
        return mSatelliteCount;
    }

    public boolean hasAlmanacData() {
        Log.d(TAG, "hasAlmanacData() called");
        return mHasAlmanacData;
    }

    public boolean hasEphemerisData() {
        Log.d(TAG, "hasEphemerisData() called");
        return mHasEphemerisData;
    }


    public int hashCode() {
        int result = mSatelliteCount;
        result = 31 * result + (mHasAlmanacData  ? 1 : 0);
        result = 31 * result + (mHasEphemerisData  ? 1 : 0);
        return result;
    }

    public int getSvCount() {
        return mSvCount;
    }

    public void setSvCount(int svCount) {
        mSvCount = svCount;
    }

    public int getBitMask() {
        return mBitMask;
    }

    public void setBitMask(int bitMask) {
        mBitMask = bitMask;
    }

    public int getTypes() {
        return mTypes;
    }

    public void setTypes(int types) {
        mTypes = types;
    }

    public float getElevationDegrees() {
        return mElevationDegrees;
    }

    public void setElevationDegrees(float elevationDegrees) {
        mElevationDegrees = elevationDegrees;
    }

    public float getAzimuthDegrees() {
        return mAzimuthDegrees;
    }

    public void setAzimuthDegrees(float azimuthDegrees) {
        mAzimuthDegrees = azimuthDegrees;
    }

    public float getSnrInDb() {
        return mSnrInDb;
    }

    public void setSnrInDb(float snrInDb) {
        mSnrInDb = snrInDb;
    }

    public int getAlmanacHealth() {
        return mAlmanacHealth;
    }

    public void setAlmanacHealth(int almanacHealth) {
        mAlmanacHealth = almanacHealth;
    }

    public int getSvHealth() {
        return mSvHealth;
    }

    public void setSvHealth(int svHealth) {
        mSvHealth = svHealth;
    }

    public int getCn0DbHz() {
        return mCn0DbHz;
    }

    public void setCn0DbHz(int cn0DbHz) {
        mCn0DbHz = cn0DbHz;
    }

    public int getFlags() {
        return mFlags;
    }

    public void setFlags(int flags) {
        mFlags = flags;
    }

    //public int getSatelliteCount() {
    //    return mSatelliteCount;
    //}

    public void setSatelliteCount(int satelliteCount) {
        mSatelliteCount = satelliteCount;
    }

    public float getSnr() {
        return mSnr;
    }

    public void setSnr(float snr) {
        mSnr = snr;
    }

    public int getConstellationType() {
        return mConstellationType;
    }

    public void setConstellationType(int constellationType) {
        mConstellationType = constellationType;
    }

    public float getCarrierFrequencyHz() {
        return mCarrierFrequencyHz;
    }

    public void setCarrierFrequencyHz(float carrierFrequencyHz) {
        mCarrierFrequencyHz = carrierFrequencyHz;
    }

    //public boolean getHasEphemerisData() {
    //    return mHasEphemerisData;
    //}

    public void setHasEphemerisData(boolean hasEphemerisData) {
        mHasEphemerisData = hasEphemerisData;
    }

    public boolean getUsedInFix() {
        return mUsedInFix;
    }

    public void setUsedInFix(boolean usedInFix) {
        mUsedInFix = usedInFix;
    }
}

